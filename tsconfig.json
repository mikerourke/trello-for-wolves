{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./lib",
    "module": "ESNext",
    "target": "es5",
    "lib": [
      "es2016",
      "es2017.object"
    ],
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": ["src"]
}
