{
  "name": "trello-for-wolves",
  "version": "2.0.0",
  "description": "Node.js wrapper for Trello API...for wolves.",
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikerourke/trello-for-wolves"
  },
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "keywords": [
    "trello",
    "api",
    "wolves",
    "wolf"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikerourke/trello-for-wolves/issues"
  },
  "homepage": "https://github.com/mikerourke/trello-for-wolves#readme",
  "scripts": {
    "build:clean": "rimraf lib",
    "tidy": "rimraf coverage && rimraf lib",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "prettier": "prettier --write \"{src,docs}/**/*.{md,ts}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "precompile": "rimraf ./build",
    "test": "jest --config jest.config.json --env=jsdom",
    "prebuild:browser": "rimraf ./dist",
    "build:browser": "parcel build src/index.ts --target browser -d dist --no-cache --global TrelloForWolves --no-content-hash --out-file trello-for-wolves.js",
    "prebuild:commonjs": "rimraf ./lib",
    "build:commonjs": "tsc -p tsconfig.build.json"
  },
  "files": [
    "dist",
    "lib",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "form-data": "^3.0.0",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/jest-in-case": "^1.0.1",
    "@types/lodash.snakecase": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "jest": "^25.1.0",
    "jest-in-case": "^1.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": false,
    "trailingComma": "all"
  }
}
